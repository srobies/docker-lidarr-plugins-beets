{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "datasourceTemplate": "docker",
      "managerFilePatterns": ["/^Dockerfile$/"],
      "matchStrings": ["FROM (?<depName>.*?):(?<currentValue>.*?)\n"]
    }
  ]
}
